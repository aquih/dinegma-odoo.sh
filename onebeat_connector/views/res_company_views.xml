<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- View res.company form -->
  <record id="view_res_company_form" model="ir.ui.view">
    <field name="name">view.res.company.form</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <page name="branches" position="after">
        <page string="Onebeat" name="onebeat">
          <group>
            <field name="is_onebeat_synchronizable"/>
          </group>
        </page>

      </page>
    </field>
  </record>

</odoo>