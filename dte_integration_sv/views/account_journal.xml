<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_account_journal_form_inh" model="ir.ui.view">
        <field name="name">account.journal.form.view.inh</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type']" position="after">
                <field name="cod_punto_venta" placeholder="Codigo Punto de Venta"/>
                <field name="secuencia_pdv_id" invisible="True"/>
            </xpath>
        </field>
    </record>


</odoo>