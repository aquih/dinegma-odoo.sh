<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="pos_payment_method_view_form_inh" model="ir.ui.view">
        <field name="name">pos.payment.method.form.view.inh</field>
        <field name="model">pos.payment.method</field>
        <field name="inherit_id" ref="point_of_sale.pos_payment_method_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='payment_method_type']" position="after">
                <field name="cod_forma_pago" placeholder="Codigo Forma de Pago"/>
            </xpath>
        </field>
    </record>


</odoo>