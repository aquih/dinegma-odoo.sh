<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_adjustment_report_export_view_form" model="ir.ui.view">
        <field name="name">stock.adjustment.report.export.view.form</field>
        <field name="model">stock.adjustment.report.export</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="start_date" />
                        </group>
                        <group>
                            <field name="end_date" />
                        </group>
                    </group>
                    <footer>
                        <button name="action_report_excel" string="EXCEL" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
